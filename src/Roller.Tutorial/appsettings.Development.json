{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cros": {
    "Enable": true,
    "PolicyName": "cros",
    "AllowAnyMethod": true,
    "AllowAnyHeader": true,
    "AllowAnyOrigin": true
  },
  "Audience": {
    "Enable": true,
    "Issuer": "test-issuer",
    "Audience": "test-audience",
    "Secret": "test-secret",
    "Expiration": 3600
  },
  "Redis": {
    "Enable": true,
    "InstanceName": "",
    "Host": "localhost",
    "Password": "password"
  },
  "Version": {
    "Enable": true,
    "HeaderName": "tutorial-api",
    "ParameterName": "tutorial-api",
    "SwaggerTitle": "tutorial-api"
  },
  "SqlSugar": {
    "Enable": true,
    "Server": "localhost",
    "Port": 3306,
    "Database": "test-db",
    "UserId": "root",
    "Password": "password",
    "SnowFlake": {
      "Enable": true,
      "WorkerId": 1
    }
  },
  "Serilog": {
    "Enable": true,
    "WriteFile": true,
    "SeqOptions": {
      "Address": "localhost",
      "Secret": "test_secret"
    }
  }
}
